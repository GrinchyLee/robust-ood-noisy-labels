{
    "name": "TIN_ResNet18_seed0",
    "n_gpu": 1,
    "seed": 0,

    "arch": {
        "type": "ResNet18_64x64",
        "args": {"num_classes":200}
    },

    "num_classes": 200,

    "data_loader": {
        "type": "TINDataloader",
        "args":{
            "data_dir": "/home/dm/Leeyujin/OpenOOD/data/images_classic",
            "batch_size": 128,
            "shuffle": true,
            "num_batches": 0,
            "validation_split": 0,
            "num_workers": 8,
            "pin_memory": true
        }
    },


    "optimizer": {
        "type": "SGD",
        "args":{
            "lr": 0.02,
            "momentum": 0.9,
            "weight_decay": 1e-3
        }
    },
    
    "train_loss": {
        "type": "elr_loss",
        "args":{
            "beta": 0.9,
            "lambda": 7
        }
    },

    "val_loss": "cross_entropy",
    "metrics": [
        "my_metric", "my_metric2"
    ],

    "lr_scheduler": {
        "type": "CosineAnnealingWarmRestarts",
        "args": {
            "T_0": 10,
            "eta_min": 0.001
        }
    },

    "trainer": {
        "epochs": 300,
        "warmup": 0,
        "save_dir": "/home/dm/Leeyujin/ELR/ELR/save/seed0",
        "save_period": 1,
        "verbosity": 2,
        "label_dir": "/home/dm/Leeyujin/ELR/ELR/save/seed0",       
        "monitor": "max val_my_metric",
        "early_stop": 2000,
        "tensorboard": false,
        "mlflow": true,
        "_percent": "Percentage of noise",
        "percent": 0.6,
        "_begin": "When to begin updating labels",
        "begin": 0,
        "_asym": "symmetric noise if false",
        "asym": false,
        "_instance": "symmetric noise if false",
        "instance": false
    }
}
